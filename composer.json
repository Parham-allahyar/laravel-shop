{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./modules/user"
        },
        {
            "type": "path",
            "url": "./modules/seller"
        },
        {
            "type": "path",
            "url": "./modules/admin"
        },
        {
            "type": "path",
            "url": "./modules/comment"
        },
        {
            "type": "path",
            "url": "./modules/category"
        },
        {
            "type": "path",
            "url": "./modules/acl"
        },
        {
            "type": "path",
            "url": "./modules/order"
        },
        {
            "type": "path",
            "url": "./modules/product"
        },
        {
            "type": "path",
            "url": "./modules/notification"
        },
        {
            "type": "path",
            "url": "./modules/address"
        },
        {
            "type": "path",
            "url": "./modules/order"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "shop/acl": "dev-master",
        "shop/address": "dev-master",
        "shop/admin": "dev-master",
        "shop/category": "dev-master",
        "shop/comment": "dev-master",
        "shop/notification": "dev-master",
        "shop/order": "dev-master",
        "shop/product": "dev-master",
        "shop/seller": "dev-master",
        "shop/user": "dev-master",
        "tymon/jwt-auth": "^1.0.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
